{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "printWidth": 80,
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "^react-dom",
    "^react-router",
    "^../pages",
    "^./pages",
    "^../components",
    "^./components",
    "^../hooks",
    "^./hooks",
    "^../api",
    "^./api",
    "^../utils",
    "^./utils",
    "^../data",
    "^./data",
    "^../types",
    "^./types",
    "^../styles",
    "^./styles",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
